<table class="spec-table" id="here_table">
  <tr>
    <th colspan='4'>Songs Index</th>
  </tr>
  <tr class="orientation">
    <td>Title</td>
    <td>Released</td>
    <td>Release Year</td>
    <td>Artist Name</td>
    <td>Genre</td>
  </tr>
<% @songs.each do |song| %>
  <tr class="orientation">
    <td><%= link_to song.title, song_path(song) %></td>
    <td><%= song.released %></td>
    <td><%= song.release_year %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.genre %></td>
  </tr>
  <% end %>

  </table>
<div>
  <button><%= link_to "New Song", new_song_path %></button>
</div>